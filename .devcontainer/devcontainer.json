{
	"name": "clab-ceis",
	"image": "mcr.microsoft.com/vscode/devcontainers/python:3",
    "postCreateCommand": ".devcontainer/postCreate.sh",
	"forwardPorts": [
		8051,
		8052
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.debugpy",
				"ms-python.vscode-pylance",
				"mhutchie.git-graph"
			]
		}
	},
    "features": {
        "ghcr.io/devcontainers/features/python:1": {
            "installTools": true
        }
    }
}
